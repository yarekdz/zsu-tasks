{
  "ConnectionStrings": {
    "Database": "Host=tasks.database;Port=5432;Database=tasks;Username=postgres;Password=postgres;Include Error Detail=true"
    //,"Cache": "runtrackr.redis:6379"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
      //,{
      //  "Name": "Seq",
      //  "Args": { "serverUrl": "http://runtrackr.seq:5341" }
      //}
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"]
  }
}
